{
  "name": "social-media-bot",
  "version": "1.5.0",
  "description": "Multi-platform Social Media Bot with Activation System",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "keep-alive": "node keep-alive.js",
    "migrate": "node migrate.js",
    "migrate:force": "node migrate.js --force",
    "migrate:seed": "node migrate.js --seed-only",
    "migrate:status": "node migrate-status.js",
    "install-deps": "npm install @whiskeysockets/baileys@6.4.0 qrcode-terminal@0.12.0 express@4.18.2 crypto@1.0.1 mysql2@3.6.0 pg@8.11.3 pg-hstore@2.3.4 sequelize@6.35.2 twitter-api-v2@1.15.0 facebook-node-sdk@0.2.0 instagram-private-api@1.45.3 discord.js@14.14.1 linkedin-api-client@0.4.0 tiktok-scraper@1.4.36 pinterest-api@1.0.6 reddit-api@0.1.0 telegram@2.15.11 socket.io@4.7.4",
    "setup": "npm run install-deps && npm run migrate",
    "docker:build": "docker build -t social-media-bot .",
    "docker:run": "docker run -p 3000:3000 --env-file .env social-media-bot",
    "docker:dev": "docker-compose up --build",
    "test": "echo \"No tests specified\" && exit 0",
    "lint": "echo \"No linter configured\" && exit 0"
  },
  "dependencies": {
    "@whiskeysockets/baileys": "6.4.0",
    "qrcode-terminal": "0.12.0",
    "express": "4.18.2",
    "@hapi/boom": "10.0.1",
    "axios": "1.6.0",
    "moment": "2.29.4",
    "fs-extra": "11.1.1",
    "
    "tiktok-scraper": "1.4.36",
    "pinterest-api": "1.0.6",
    "reddit-api": "0.1.0",
    "telegram": "2.15.11",
    "socket.io": "4.7.4",
    "mysql2": "3.6.0"
  },
  "devDependencies": {
    "nodemon": "3.0.1"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "keywords": [
    "whatsapp",
    "bot",
    "facebook",
    "twitter",
    "instagram",
    "discord",
    "social-media",
    "multi-platform"
  ],
